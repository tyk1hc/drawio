        <mxCell id="MlYqjOnE9EuYvu6xzNQ--82" value="&lt;font style=&quot;font-size: 14px;&quot;&gt;Improvements to the Memento Pattern&lt;/font&gt;" style="text;whiteSpace=wrap;html=1;fontStyle=1" vertex="1" parent="1">
